#!/usr/bin/env ruby

require 'bundler/setup'
require 'pry'
require 'faker'
require 'simple_csv'

SimpleCsv.read('spec/files/headerless.csv', has_headers: false) do
  headers :first_name, :last_name, :employed_at, :birth_date

  each_row do
    p birth_date
  end
end


